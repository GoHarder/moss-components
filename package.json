    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "highlight.js": "^11.11.1",
    "highlightjs-svelte": "^1.0.6",
    "publint": "^0.3.2",
    "sass-embedded": "^1.85.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
